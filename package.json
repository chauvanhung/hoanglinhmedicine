{
  "name": "hoanglinh-obesity-care",
  "version": "1.0.0",
  "description": "Obesity Care Application - Full Stack",
  "private": true,
  "scripts": {
    "dev": "cd apps/web && npm run dev",
    "build": "cd apps/web && npm run build",
    "start": "cd apps/web && npm start",
    "build:api": "cd apps/api && npm run build",
    "start:api": "cd apps/api && npm run start:prod",
    "install:all": "npm install && cd apps/web && npm install && cd ../api && npm install"
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
