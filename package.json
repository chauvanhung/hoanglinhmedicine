{
  "name": "hoanglinh-obesity-care",
  "version": "1.0.0",
  "description": "Ứng dụng chăm sóc béo phì với AI coaching, tư vấn bác sĩ và theo dõi sức khỏe",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "db:migrate": "cd apps/api && pnpm db:migrate",
    "db:seed": "cd apps/api && pnpm db:seed",
    "db:studio": "cd apps/api && pnpm db:studio"
  },
  "devDependencies": {
    "turbo": "^1.10.16",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.6.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
